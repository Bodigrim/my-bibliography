@misc{1307.3683v1,
	abstract = "Consider an operator E on arithmetic functions such that $Ef$ is multiplicative function and $(Ef)(p^a) = f(a)$. We investigate behaviour of $E^m\tau_k$, where $\tau_k$ is a k-dimensional divisor function, and estimate error terms of $\sum_{n\le x} E^m\tau_k(n)$ for various combinations of m and k. We also study properties of $E^mf$ for arbitrary f and sufficiently large m.",
	archiveprefix = "arXiv",
	author = "A. V. Lelechenko",
